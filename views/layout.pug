          #user-dropdown.z-50.hidden.my-4.text-base.list-none.bg-white.divide-y.divide-gray-100.rounded-lg.shadow(class='dark:bg-gray-700 dark:divide-gray-600')
            .px-4.py-3
              span.block.text-sm.text-gray-900(class='dark:text-white') Bonnie Green
              span.block.text-sm.text-gray-500.truncate(class='dark:text-gray-400') name@flowbite.com
            ul.py-2(aria-labelledby='user-menu-button')
              li
                a.block.px-4.py-2.text-sm.text-gray-700(href='#' class='hover:bg-gray-100 dark:hover:bg-gray-600 dark:text-gray-200 dark:hover:text-white') Dashboard
