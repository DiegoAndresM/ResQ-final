            img.w-6.h-6.rounded-full(src='https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSuw6eoLg-LJVsqcG8VltKMdFMakOSVn4eB1VxdLeY49w&s' alt='user photo')
